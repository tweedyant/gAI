{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "buildCommand": "pnpm start --characters=\"characters/gai.character.json\""
  }
